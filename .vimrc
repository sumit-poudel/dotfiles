
filetype plugin indent on

set mouse=a
set number
set relativenumber
set termguicolors
syntax on
colorscheme catppuccin_mocha

" nvim defaults
set tabstop=4        
set shiftwidth=4     
set expandtab        

set hlsearch "hilight search, :nohl to remove

set t_Co=256 "terminal colors

set wildmenu " auto completion, trigger with <C-n> 
